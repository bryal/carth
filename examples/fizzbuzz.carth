(import std)

(defvar main fizzbuzz)

(defvar fizzbuzz
  (io/for (range 1 100)
          (<o display fizzbuzz')))

(defun fizzbuzz' [n]
  (match [(divisible? n 3) (divisible? n 5)]
    (case [False False] (my-show-int n))
    (case [True  False] "Fizz")
    (case [False True ] "Buzz")
    (case [True  True ] "Fizzbuzz")))

(defun my-show-int
  (case [1] "one")
  (case [2] "two")
  (case [n] (show-int n)))
