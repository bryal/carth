(import std)

(def main
  (display (show-nat (ackermann (cast 3) (cast 10)))))

(defun ackermann [m n] :of (Fun [Nat Nat] Nat)
  (if (= m (cast 0))
      (+ n (cast 1))
    (if (= n (cast 0))
        (ackermann (- m (cast 1)) (cast 1))
      (ackermann (- m (cast 1)) (ackermann m (- n (cast 1)))))))
